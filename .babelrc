{
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "decoratorsBeforeExport": false }],
    ["@babel/plugin-syntax-dynamic-import"]
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
        "ie": 11
        },
        "useBuiltIns": "usage",
        "corejs": {
          "version": 3,
          "proposals": true
        }
      }
    ]
  ]
}
